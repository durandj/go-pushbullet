{
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
  },
  "go.lintTool": "golangci-lint",
  "go.useLanguageServer": true,
  "go.alternateTools": {
      "go-langserver": "gopls"
  },
  "go.autocompleteUnimportedPackages": true,
  "cSpell.words": [
    "ephemerals",
    "iden",
    "pushbullet",
    "stretchr"
  ]
}
